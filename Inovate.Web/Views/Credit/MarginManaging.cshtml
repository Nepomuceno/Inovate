
@{
    ViewBag.Title = "MarginManaging";
}

<h2>Colocações</h2>
<div class="row">
    <form class="form-horizontal" role="form">
        <div class="form-group">
            <label for="inputEmail1" class="col-lg-2 control-label">Margem</label>
            <div class="col-lg-10">
                <input type="number" name="currency" min="0" max="20" step="0.01" value="10" size="4" /> %
            </div>
        </div>
       

        <div class="form-group">
            <div class="col-lg-offset-2 col-lg-10">
                <button type="submit" class="btn btn-default">Salvar</button>
            </div>
        </div>
    </form>
</div>


